        ]
    },
    {
        "id": "256832ea2fdea91f",
        "type": "function",
        "z": "429937dcb088aa8e",
        "name": "Contar por Setor",
        "func": "// Armazena estado dos sensores\ncontext.sensores = context.sensores || {};\nlet sensor = msg.payload;\n\ncontext.sensores[sensor.id_sensor] = sensor.setor;\n\n// Conta sensores por setor\nlet contagem = {};\nfor (let id in context.sensores) {\n    let s = context.sensores[id];\n    contagem[s] = (contagem[s] || 0) + 1;\n}\n\n// Cria uma mensagem separada para cada setor\nlet output = [];\nfor (let setor in contagem) {\n    output.push({ payload: contagem[setor], topic: setor });\n}\n\nreturn output.map(m => [m]);",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
